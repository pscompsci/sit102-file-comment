{
    "name": "sit102-file-comment",
    "displayName": "sit102-file-comment",
    "description": "The easiest way to insert file comments and docstrings into C/C++ and Python files for SIT102",
    "version": "0.0.1",
    "publisher": "Peter Stacey",
    "engines": {
        "vscode": "^1.40.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pscompsci/sit102-file-comment.git"
    },
    "keywords": [
        "Deakin University",
        "SIT102"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sit102b"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.sit102b",
                "title": "SIT102 Exercise File Comment"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "SIT102",
            "properties": {
                "student.name": {
                    "type": "string",
                    "default": "Student name (edit in settings)",
                    "description": "Student name"
                },
                "student.id": {
                    "type": "string",
                    "default": "Student ID (edit in settings)",
                    "description": "Student ID number"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.1",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.42.0",
        "eslint": "^6.8.0",
        "@typescript-eslint/parser": "^2.18.0",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "glob": "^7.1.6",
        "mocha": "^7.0.1",
        "typescript": "^3.7.5",
        "vscode-test": "^1.3.0"
    }
}